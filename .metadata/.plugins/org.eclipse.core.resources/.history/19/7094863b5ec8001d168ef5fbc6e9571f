	public static void main(String[] args) {
		
		while (true) {
			List<User> userList = new ArrayList<>();
			// 모두 입력
			if () {
				
				continue;
			}
			
			// 존재하는 이메일?
			if () {
				
				continue;
			}
			
			// 비밀번호가 서로 같은지?
			if (userList.equals(email)) {
				
				continue;
			}
			
			User user = new User();
			
			
		}
	}
}
