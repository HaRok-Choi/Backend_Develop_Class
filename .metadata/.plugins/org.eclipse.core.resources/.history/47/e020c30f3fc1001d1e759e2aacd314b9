package trainReservation.entity;

import java.util.List;

// 기차 정보 Entity class
public class Train {
	String trainNumber;	// 열차번호
	String departureTimes;	// 출발시간
	String arrivalTime;	// 도착시간
	int takeMinute; // 걸리는 시간(분 단위)
	String type; // 기차종류
	
	List<StopStation>
	
//	남아있는 좌석 (class -> 호차, 좌석번호, 좌석상태)
	
}
